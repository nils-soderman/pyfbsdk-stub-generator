{
    "files.exclude": {
        "**/.git": true,
        "**/__pycache__/**": true,
        "*.egg-info": true,
        "**/env": true,
        "dist": true,
        "build": true,
        "LICENSE": true,
    },
    "search.exclude": {
        "generated-stub-files": true,
    },
    "autopep8.args": [
        "--ignore=E501,E27,E251,E402"
    ],
    "python.analysis.typeCheckingMode": "basic",
    "python.analysis.extraPaths": [
        "./generated-stub-files/motionbuilder-2025",
        "./env",
    ],
}